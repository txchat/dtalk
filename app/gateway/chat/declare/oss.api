// goctl api format -dir .

type LoginReq {
	ConnType int32 `json:"connType"`
}

type LoginResp {
    Address string `json:"address"`
}

@server(
    group: oss
    middleware: AppAuthMiddleware
)
service chat-api {
    @handler Login
    post /app/user/login (LoginReq) returns (LoginResp)
}