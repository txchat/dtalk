COMPOSE_PROJECT_NAME=txchat_biz

## MYSQL
# mysql版本号
MYSQL_VERSION=8-oracle
# mysql容器名称
MYSQL_CONTAINER_NAME=txchat-mysql
# mysql数据卷
MYSQL_DATA_VOLUME=txchat-mysql-data
# mysql配置卷
MYSQL_CONFIG_VOLUME=txchat-mysql-config
# mysql日志卷
MYSQL_LOG_VOLUME=txchat-mysql-log
# mysql初始化卷
MYSQL_INIT_VOLUME=txchat-mysql-init
# mysql端口号
MYSQL_REAL_PORT=3306
# mysql超级用户密码
MYSQL_ROOT_PASSWORD=${MYSQL_ROOT_PASSWORD}
# mysql超级用户允许地址
MYSQL_ROOT_HOST=%

## MINIO
# minio版本号
MINIO_VERSION=RELEASE.2021-07-22T05-23-32Z
# minio容器名称
MINIO_CONTAINER_NAME=txchat-minio
# minio数据卷
MINIO_DATA_VOLUME=txchat-minio-data
# minio超级用户账户
MINIO_ROOT_USER=minio
# minio超级用户密码
MINIO_ROOT_PASSWORD=${MINIO_ROOT_PASSWORD}

