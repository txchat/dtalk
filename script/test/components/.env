COMPOSE_PROJECT_NAME=txchat_test_biz

## ZOOKEEPER
# zookeeper版本号
ZOOKEEPER_VERSION=latest
# zookeeper容器名称
ZOOKEEPER_CONTAINER_NAME=txchat-test-zookeeper
# zookeeper数据卷
ZOOKEEPER_DATA_VOLUME=txchat-test-zookeeper-data

## KAFKA
# kafka版本号
KAFKA_VERSION=2.13-2.8.1
# kafka容器名称
KAFKA_CONTAINER_NAME=txchat-test-kafka
# kafka数据卷
KAFKA_DATA_VOLUME=txchat-test-kafka-data
# 宿主机ip
HOST_ETH0_NAME=127.0.0.1

## KAFKA_MANAGER
# kafka容器名称
KAFKA_MANAGER_CONTAINER_NAME=txchat-test-kafka-manager

## REDIS
# redis版本号
REDIS_VERSION=6.2.1
# redis容器名称
REDIS_CONTAINER_NAME=txchat-test-redis
# redis数据卷
REDIS_DATA_VOLUME=txchat-test-redis-data
# redis配置卷
REDIS_CONFIG_VOLUME=txchat-test-redis-config
# redis日志卷
REDIS_LOG_VOLUME=txchat-test-redis-log
# redis端口号
REDIS_PUB_PORT=127.0.0.1:6379
# redis连接密码
REDIS_PASSWORD=123456

## ETCD
# etcd版本号
ETCD_VERSION=v3.5.4
# etcd容器名称
ETCD_CONTAINER_NAME=txchat-test-etcd
# etcd数据卷
ETCD_DATA_VOLUME=txchat-test-etcd-data
# etcd客户端连接端口号
ETCD_PUB_CLIENT_PORT=2379
# etcd节点连接端口号
ETCD_PUB_PEER_PORT=2380
# etcd节点名称
ETCD_NAME=infra1
# etcd初始化令牌
ETCD_TOKEN=token

## MYSQL
# mysql版本号
MYSQL_VERSION=8-oracle
# mysql容器名称
MYSQL_CONTAINER_NAME=txchat-test-mysql
# mysql数据卷
MYSQL_DATA_VOLUME=txchat-test-mysql-data
# mysql配置卷
MYSQL_CONFIG_VOLUME=txchat-test-mysql-config
# mysql日志卷
MYSQL_LOG_VOLUME=txchat-test-mysql-log
# mysql初始化卷
MYSQL_INIT_VOLUME=txchat-test-mysql-init
# mysql端口号
MYSQL_REAL_PORT=3306
# mysql超级用户密码
MYSQL_ROOT_PASSWORD=123456
# mysql超级用户允许地址
MYSQL_ROOT_HOST=%

## MINIO
# minio版本号
MINIO_VERSION=RELEASE.2021-07-22T05-23-32Z
# minio容器名称
MINIO_CONTAINER_NAME=txchat-test-minio
# minio数据卷
MINIO_DATA_VOLUME=txchat-test-minio-data
# minio超级用户账户
MINIO_ROOT_USER=minio
# minio超级用户密码
MINIO_ROOT_PASSWORD=minio

## NGINX
# nginx版本号
NGINX_VERSION=1.19.2-alpine
# nginx容器名称
NGINX_CONTAINER_NAME=txchat-test-nginx
# nginx配置卷
NGINX_CONFIG_VOLUME=txchat-test-nginx-config
# nginx日志卷
NGINX_LOG_VOLUME=txchat-test-nginx-log

## MONGO
# mongo版本号
MONGO_VERSION=latest
# mongo容器名称
MONGO_CONTAINER_NAME=txchat-test-mongo
MONGO_EXPRESS_CONTAINER_NAME=txchat-test-mongo-express
# mongo配置卷
MONGO_CONFIG_VOLUME=txchat-test-mongo-config
# mongo数据卷
MONGO_DATA_VOLUME=txchat-test-mongo-data
