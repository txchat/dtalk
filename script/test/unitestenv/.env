COMPOSE_PROJECT_NAME=txchat_test_uni

## REDIS
# redis版本号
REDIS_VERSION=6.2.1
# redis容器名称
REDIS_CONTAINER_NAME=${COMPOSE_PROJECT_NAME}-redis
# redis数据卷
REDIS_DATA_VOLUME=${COMPOSE_PROJECT_NAME}-redis-data
# redis配置卷
REDIS_CONFIG_VOLUME=${COMPOSE_PROJECT_NAME}-redis-config
# redis日志卷
REDIS_LOG_VOLUME=${COMPOSE_PROJECT_NAME}-redis-log
# redis端口号
REDIS_PUB_PORT=127.0.0.1:6379
# redis连接密码
REDIS_PASSWORD=123456

## MYSQL
# mysql版本号
MYSQL_VERSION=8-oracle
# mysql容器名称
MYSQL_CONTAINER_NAME=${COMPOSE_PROJECT_NAME}-mysql
# mysql数据卷
MYSQL_DATA_VOLUME=${COMPOSE_PROJECT_NAME}-mysql-data
# mysql配置卷
MYSQL_CONFIG_VOLUME=${COMPOSE_PROJECT_NAME}-mysql-config
# mysql日志卷
MYSQL_LOG_VOLUME=${COMPOSE_PROJECT_NAME}-mysql-log
# mysql初始化卷
MYSQL_INIT_VOLUME=${COMPOSE_PROJECT_NAME}-mysql-init
# mysql端口号
MYSQL_REAL_PORT=3306
# mysql超级用户密码
MYSQL_ROOT_PASSWORD=123456
# mysql超级用户允许地址
MYSQL_ROOT_HOST=%

## MONGO
# mongo版本号
MONGO_VERSION=latest
# mongo容器名称
MONGO_CONTAINER_NAME=${COMPOSE_PROJECT_NAME}-mongo
MONGO_EXPRESS_CONTAINER_NAME=${COMPOSE_PROJECT_NAME}-mongo-express
# mongo配置卷
MONGO_CONFIG_VOLUME=${COMPOSE_PROJECT_NAME}-mongo-config
# mongo数据卷
MONGO_DATA_VOLUME=${COMPOSE_PROJECT_NAME}-mongo-data
